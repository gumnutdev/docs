{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Gumnut Docs",
  "description": "Documentation for Gumnut",
  "colors": {
    "primary": "#DA4F9A",
    "light": "#EA9AC4",
    "dark": "#972060"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart", "walkthrough"]
          },
          {
            "group": "Guides",
            "pages": ["guides/authentication"]
          },
          {
            "group": "Components",
            "pages": [
              {
                "group": "Frameworks",
                "icon": "code",
                "pages": ["components/react", "components/vue"]
              },
              {
                "group": "Components",
                "icon": "code",
                "pages": [
                  "components/gumnut-data",
                  "components/gumnut-status",
                  "components/gumnut-text"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/gumnut-api",
              "api-reference/gumnut-doc",
              "api-reference/gumnut-node"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/real-time-collaboration",
              "concepts/operational-transformation"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.gg/yu3u87AUNR",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.gumnut.dev",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png",
    "href": "https://gumnut.dev"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@gumnut.dev"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/gumnutdev",
      "linkedin": "https://www.linkedin.com/company/gumnut-dev/"
    }
  }
}
