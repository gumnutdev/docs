{
  "hosting": [
    {
      "target": "blog",
      "public": "docs/.vitepress/dist",
      "ignore": [],
      "cleanUrls": true,
      "trailingSlash": false,
      "rewrites": [
        {
          "source": "/blog",
          "destination": "/blog.html"
        },
        {
          "source": "/case-studies",
          "destination": "/case-studies.html"
        },
        {
          "source": "/pricing",
          "destination": "/pricing.html"
        },
        {
          "source": "/questions",
          "destination": "/questions.html"
        },
        {
          "source": "/articles/**",
          "destination": "/articles/**"
        },
        {
          "source": "/case-studies/**",
          "destination": "/case-studies/**"
        },
        {
          "source": "/docs/**",
          "destination": "/docs/**"
        },
        {
          "source": "/",
          "destination": "/index.html"
        },
        {
          "source": "**",
          "destination": "/404.html"
        }
      ]
    }
  ]
}
