{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Gumnut Docs",
  "description": "Documentation for Gumnut",
  "colors": {
    "primary": "#DA4F9A",
    "light": "#EA9AC4",
    "dark": "#972060"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart", "complete-guide"]
          },
          {
            "group": "Guides",
            "pages": ["guides/authentication", "guides/collaborative-editing"]
          },
          {
            "group": "Components",
            "pages": [
              {
                "group": "Frameworks",
                "icon": "code",
                "pages": [
                  "components/react",
                  "components/vue",
                  "components/svelte"
                ]
              },
              {
                "group": "Components",
                "icon": "code",
                "pages": [
                  "components/gumnut-data",
                  "components/gumnut-status",
                  "components/gumnut-textarea",
                  "components/gumnut-input",
                  "components/gumnut-button",
                  "components/gumnut-checkbox",
                  "components/gumnut-radio",
                  "components/gumnut-select",
                  "components/gumnut-slider"
                ]
              }
            ]
          }
        ]
      },
      {
        "dropdown": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/gumnut-api",
              "api-reference/gumnut-doc",
              "api-reference/gumnut-node"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/real-time-collaboration",
              "concepts/operational-transformation"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.gg/yu3u87AUNR",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.gumnut.dev",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png",
    "href": "https://gumnut.dev"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@gumnut.dev"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/gumnutdev",
      "linkedin": "https://www.linkedin.com/company/gumnut-dev/"
    }
  }
}
